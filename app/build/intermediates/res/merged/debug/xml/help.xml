    android:title="@string/app_name"
    android:summary="https://github.com/winnerawan/adukerang-master"/>
</co.ipb.adukerang.helper.KerangPrefScreen>

<co.ipb.adukerang.helper.KerangPrefScreen android:title="@string/help0">
<Preference
    android:key="gpl3"
    android:title="@string/app_name"
    android:summary="Panduan"/>
<Preference
    android:key="apache2"
    android:title="AduKerang"
    android:summary="Version App"/>
</co.ipb.adukerang.helper.KerangPrefScreen>
</PreferenceScreen>