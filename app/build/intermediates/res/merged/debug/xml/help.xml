<PreferenceScreen
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <co.ipb.adukerang.helper.KerangPrefScreen
        android:id="@+id/aaa"
        android:title="A" android:textSize="@dimen/font_mll">

    </co.ipb.adukerang.helper.KerangPrefScreen>
    <co.ipb.adukerang.helper.KerangPrefScreen
        android:id="@+id/aaa"
        android:title="" android:textSize="@dimen/font_mll">

    </co.ipb.adukerang.helper.KerangPrefScreen>
<co.ipb.adukerang.helper.KerangPrefScreen
    android:layout_below="@id/aaa"
    android:title="@string/category_author"
    android:paddingTop="10dip"
    android:layout_marginBottom="10dip"
    android:layout_marginTop="14dip">
    <Preference
        android:key="author"
        android:title="Diyah Putri"
        android:summary="@string/credits_author_summary"/>
</co.ipb.adukerang.helper.KerangPrefScreen>

<co.ipb.adukerang.helper.KerangPrefScreen android:title="@string/category_source_code">
<Preference
